.highlight {
  font-family: Verdana, Helvetica, sans-serif;
  font-weight: bold;
  font-size: 14px;
  text-align: center;o 
}

.highlight a, .highlight a:hover {
  text-decoration: none;
}

.highlight > div {
  border-radius: 6px;
  border: 1px solid #bbb;
  padding: 4px;
}

.highlight .red_gradient {
  background-color: #b91c16;
  background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#e46560), to(#b91c16));
  background-image: -webkit-linear-gradient(top, #e46560, #b91c16);
  background-image:    -moz-linear-gradient(top, #e46560, #b91c16);
  background-image:     -ms-linear-gradient(top, #e46560, #b91c16);
  background-image:      -o-linear-gradient(top, #e56560, #b91c16);
  filter: progid:DXImageTransform.Microsoft.Gradient(GradientType=0,startColorstr=#e46560, endColorstr=#b91c16);
}

.highlight .red_gradient:hover {
  background-color: #b91c16;
  background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#b91c16), to(#e46560));
  background-image: -webkit-linear-gradient(top, #b91c16, #e46560);
  background-image:    -moz-linear-gradient(top, #b91c16, #e46560);
  background-image:     -ms-linear-gradient(top, #b91c16, #e46560);
  background-image:      -o-linear-gradient(top, #b91c16, #e56560);
  filter: progid:DXImageTransform.Microsoft.Gradient(GradientType=0,startColorstr=#b91c16, endColorstr=#e46560);
}

.highlight .yellow_gradient {
  background-color: #FF9F00;
  background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#FFCC00), to(#FF9F00));
  background-image: -webkit-linear-gradient(top, #FFCC00, #FF9F00); 
  background-image:    -moz-linear-gradient(top, #FFCC00, #FF9F00);
  background-image:     -ms-linear-gradient(top, #FFCC00, #FF9F00);
  background-image:      -o-linear-gradient(top, #FFCC00, #FF9F00);
  filter: progid:DXImageTransform.Microsoft.Gradient(GradientType=0,startColorstr=#FFCC00, endColorstr=#FF9F00);
}

.highlight .yellow_gradient:hover {
  background-color: #FF9F00;
  background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#FF9F00), to(#FFCC00));
  background-image: -webkit-linear-gradient(top, #FF9F00, #FFCC00);
  background-image:    -moz-linear-gradient(top, #FF9F00, #FFCC00);
  background-image:     -ms-linear-gradient(top, #FF9F00, #FFCC00);
  background-image:      -o-linear-gradient(top, #FF9F00, #FFCC00);
  filter: progid:DXImageTransform.Microsoft.Gradient(GradientType=0,startColorstr=#FF9F00, endColorstr=#FFCC00);
}

.highlight .white_gradient {
  background-color: #FFFFFF;
  background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#f5f5f5), to(#ccccce));
  background-image: -webkit-linear-gradient(top, #f5f5f5, #ccccce);
  background-image:    -moz-linear-gradient(top, #f5f5f5, #ccccce);
  background-image:     -ms-linear-gradient(top, #f5f5f5, #ccccce);
  background-image:      -o-linear-gradient(top, #f5f5f5, #ccccce);
  filter: progid:DXImageTransform.Microsoft.Gradient(GradientType=0,startColorstr=#f5f5f5, endColorstr=#ccccce);
}

.highlight .white_gradient:hover {
  background-color: #FFFFFF;
  background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#ccccce), to(#f5f5f5));
  background-image: -webkit-linear-gradient(top, #ccccce, #f5f5f5);
  background-image:    -moz-linear-gradient(top, #ccccce, #f5f5f5);
  background-image:     -ms-linear-gradient(top, #ccccce, #f5f5f5);
  background-image:      -o-linear-gradient(top, #ccccce, #f5f5f5);
  filter: progid:DXImageTransform.Microsoft.Gradient(GradientType=0,startColorstr=#ccccce, endColorstr=#f5f5f5);
}

.highlight .black_gradient {
  background-color: #000000;
  background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#555551), to(#0a0a0a));
  background-image: -webkit-linear-gradient(top, #555551, #0a0a0a);
  background-image:    -moz-linear-gradient(top, #555551, #0a0a0a);
  background-image:     -ms-linear-gradient(top, #555551, #0a0a0a);
  background-image:      -o-linear-gradient(top, #555551, #0a0a0a);
  filter: progid:DXImageTransform.Microsoft.Gradient(GradientType=0,startColorstr=#555551, endColorstr=#0a0a0a);
}

.highlight .black_gradient:hover {
  background-color: #000000;
  background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#0a0a0a), to(#555551));
  background-image: -webkit-linear-gradient(top, #0a0a0a, #555551);
  background-image:    -moz-linear-gradient(top, #0a0a0a, #555551);
  background-image:     -ms-linear-gradient(top, #0a0a0a, #555551);
  background-image:      -o-linear-gradient(top, #0a0a0a, #555551);
  filter: progid:DXImageTransform.Microsoft.Gradient(GradientType=0,startColorstr=#0a0a0a, endColorstr=#555551);
}

.highlight .black_text {
  color: black;
}

.highlight .blue_text {
  color: #2480bd;
}

.highlight .gray_text {
  color: gray;
}

.highlight .white_text {
  color: white;
}

.highlight .drop_shadow {
  text-shadow: 1px 1px 2px #FFFFFF;
}

